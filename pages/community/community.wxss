/* pages/community/community.wxss */
page{
  width: 100%;
  margin: 0;
  background: #f6f6f6;
}
.content{
  display: flex;
  flex-direction: row;
  overflow: hidden;
}
.img_item {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  margin: 0rpx 10rpx 20rpx 15rpx;
    width: 350rpx;
}
.aligncenter{
  display: flex;
  align-items: center;
}
.imgBox{
  width: calc(100%/2);
  width: 350rpx;
  background: #fff;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
  padding-bottom: 20rpx;
  margin-right: 10rpx;
}
.imgUrl{
  max-height: 480rpx;
  max-width: 350rpx;
  border-radius: 10rpx 10rpx 0 0;
}

