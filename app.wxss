.noDataBox{
  text-align: center;
  height: 100%;
  background: #f6f6f6;
}
.noDataBox .nodataImg{
    height: 308rpx;
    width: 394rpx;
    margin: 80rpx auto 0rpx;
}
.centerBox{
  align-items: center;
}
.flexwrap{
  display: flex;
  flex-wrap: wrap;
}
.clear:after {
    content: '';
    height: 0;
    line-height: 0;
    display: block;
    visibility: hidden;
    clear: both;
}
